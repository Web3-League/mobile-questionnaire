<!DOCTYPE hibernate-reverse-engineering PUBLIC
    "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN"
    "http://hibernate.org/dtd/hibernate-reverse-engineering-3.0.dtd">
<hibernate-reverse-engineering>
	<type-mapping>
		<sql-type jdbc-type="VARCHAR" hibernate-type="string"></sql-type>
	</type-mapping>
	<!-- Target the correct schema -->
	<schema-selection match-schema="cosmetest" />

	<!-- Optional: Include all tables in the schema -->
	<table-filter match-name="%" />
	<table-filter match-name=".*"></table-filter>
	<table-filter match-name=".*"></table-filter>
	<table-filter match-name=".*"></table-filter>
	<table-filter match-schema="cosmetest" match-name=".*"></table-filter>
	<table-filter match-name="annulation" match-catalog="cosmetest"></table-filter>
	<table-filter match-name="annulation" match-catalog="cosmetest"></table-filter>
	<table-filter match-name="etude" match-catalog="cosmetest"></table-filter>
</hibernate-reverse-engineering>
